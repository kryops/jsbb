{
  "name": "jsbb",
  "version": "0.0.1",
  "description": "JavaScript Browser Benchmark",
  "main": "index.html",
  "dependencies": {
    "@angular/common": "2.0.0-rc.5",
    "@angular/compiler": "2.0.0-rc.5",
    "@angular/compiler-cli": "0.5.0",
    "@angular/core": "2.0.0-rc.5",
    "@angular/forms": "0.3.0",
    "@angular/http": "2.0.0-rc.5",
    "@angular/platform-browser": "2.0.0-rc.5",
    "@angular/platform-browser-dynamic": "2.0.0-rc.5",
    "@angular/router": "3.0.0-rc.1",
    "ace-editor-builds": "^1.2.4",
    "benchmark": "^2.1.1",
    "core-js": "^2.4.0",
    "lodash": "^4.15.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "^5.0.0-beta.11",
    "rxjs-es": "^5.0.0-beta.11",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "@types/ace": "0.0.31",
    "@types/benchmark": "^1.0.29",
    "@types/lodash": "^4.14.34",
    "rollup": "^0.34.10",
    "rollup-plugin-node-resolve": "^2.0.0",
    "typescript": "^2.0.0",
    "uglifyjs": "^2.4.10",
    "watch": "^0.19.2"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "tsc:es5": "tsc --target es5 --allowJs dist/temp/rollup/bundle.js --outDir dist/temp/es5",
    "concat": "for filename in node_modules/ace-editor-builds/src-min-noconflict/ace.js node_modules/ace-editor-builds/src-min-noconflict/mode-javascript.js node_modules/core-js/client/shim.min.js node_modules/zone.js/dist/zone.js node_modules/reflect-metadata/Reflect.js  node_modules/lodash/lodash.min.js node_modules/benchmark/benchmark.js dist/temp/es5/bundle.js; do cat \"${filename}\"; echo;  done > dist/temp/complete.bundle.js",
    "uglify": "uglifyjs dist/temp/complete.bundle.js --screw-ie8 --compress --output dist/bundle.min.js",
    "skip_uglify": "cat dist/temp/complete.bundle.js > dist/bundle.min.js",
    "build": "npm run clean && tsc && rollup -c && npm run tsc:es5 && npm run concat && npm run uglify",
    "watch": "watch 'npm run build' --wait=10 --interval=10 -d -p='/dist|node_modules|.idea/'"
  },
  "author": "Michael Strobel",
  "license": "MIT"
}
